
@model IEnumerable<AsteriskBrowserAL.Models.ExaminationPapersData>
@{
    Layout = "~/Views/Shared/Parent_Layout.cshtml";
}
@{
    ViewBag.Title = "CicularsPartialView";

    int nSNo = 1;
}


@if (null != Model && Model.Count() > 0)
{
    <div class="col-xs-12">
        <div class="table-header">
            <div class="txtTableHeader">
               Circulars (@Model.Count())
            </div>
        </div>
        <table class="table tbhover table-striped table-bordered dynamic-table">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayName("Sno")
                    </th>

                    <th>
                        @Html.DisplayName("Date")
                    </th>
                    <th>
                        @Html.DisplayName("Circular Type")
                    </th>
                    <th>
                        @Html.DisplayName("Circular Name")
                    </th>
                    <th>
                        @Html.DisplayName("Download Circular")
                    </th>
                    @*<th>
                @Html.DisplayName("Video Link")
            </th>*@
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @(nSNo++)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.StartTime)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ExamCode)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.ExamName)
                        </td>
                        <td>
                            <a href="@item.strExamDate" target="_blank">Download</a>
                        </td>
                        @*<td>
                    @Html.DisplayFor(modelItem => item.ExamCode)
                </td>*@

                    </tr>

                }
            </tbody>
        </table>
    </div>

}
else
{
    <div class="col-xs-12">
        <span style="font-weight:bold;color:red">Data not available to display</span>
    </div>
}
@Html.Action("DataTableScripts", "Base")
