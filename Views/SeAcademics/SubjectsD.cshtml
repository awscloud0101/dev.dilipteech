
@model AsteriskBrowserAL.Subjects

@{
    ViewBag.Title = "SubjectsD";
}
@section BreadCrumb{

    <ul class="breadcrumb">
        <li>
            <i class="menu-icon fa fa-list"></i>
            Setting
        </li>
        <li>
            <i class=""></i>
            Academics
        </li>
        <li class="active">Subjects</li>
    </ul><!-- /.breadcrumb -->
}






<section class="content">
    <div class="row">
        <div class=col-xs-12>
            <h3> @Html.ActionLink("<< Subjects Details", "Subjects", new { BatchId = ViewBag.selectd_BatchId })</h3>
        </div>
    </div>
   
    <br />
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                @using (Html.BeginForm("SubjectsD", "SeAcademics", FormMethod.Post, new { @class = "form-horizontal" }))
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true)
                    <span style="color: red;">@Html.ValidationMessage("Subjects")</span>

                    <div class="box-header">
                        <h3 class="box-title">Subjects</h3>
                        
                        <button class="btn btn-primary" type="submit" style="width:80px;float:right">
                            Delete
                        </button>
                    </div><!-- /.box-header -->
                    <div class="box-body table-responsive no-padding">
                        <table class="table table-hover">
                            <tbody>
                                <tr>

                                    <th>
                                        @Html.DisplayName("Subject Name")
                                    </th>
                                    <th>
                                        @Html.DisplayName("Subject Code")
                                    </th>

                                </tr>
                                <tr>
                                    <td>
                                        @Html.DisplayFor(model => model.SubjectName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(model => model.SubjectCode)
                                    </td>

                                </tr>

                            </tbody>
                        </table>
                    </div>
                }
            </div>
        </div>
    </div>
</section>