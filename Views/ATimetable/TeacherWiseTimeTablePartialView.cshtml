@model List<AsteriskBrowserAL.Models.TeacherWiseTimeTableData>

@{
    int SNo = 1;
}

<br />
<div>
   

    <div id="dialog-confirm" class="hide">

        <div class="space-6"></div>

        <p id="dValidation" class="bigger-120 bolder center grey">
        </p>
    </div>
    <div class="hr hr-18 dotted hr-double"></div>

    @if (null != Model && Model.Count() > 0)
    {
       
            <div class="row">
                <div class="col-xs-12">
                    <div class="box">
                        <div class="box-header">
                            <div class="table-header">
                                Records Found (@Model.Count())
                            </div>
                            <div class="box-body">
                                <div class="box-body table-responsive no-padding">

                                    <div class="hideScrolling">
                                        <table class="table tbhover table-striped table-bordered dynamic-table" style="font-size:14px;overflow:auto;">
                                            <thead>
                                                <tr>
                                                    <th>
                                                        @Html.DisplayName("S.No")
                                                    </th>

                                                    <th>
                                                        @Html.DisplayName("Period")
                                                    </th>

                                                    <th>
                                                        @Html.DisplayName("ItemStartTime")
                                                    </th>
                                                    <th>
                                                        @Html.DisplayName("ItemEndTime")
                                                    </th>
                                                    <th>
                                                        @Html.DisplayName("SubjectName")
                                                    </th>
                                                   
                                                   
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var s in Model)
                                                {
                                                    <tr>
                                                        <td>
                                                            @SNo
                                                        </td>
                                                        <td>

                                                            @s.ItemName


                                                        </td>
                                                        <td>
                                                           @s.ItemStartTime
                                                        </td>

                                                        <td>
                                                          @s.ItemEndTime
                                                        </td>

                                                       

                                                        <td>
                                                            @s.SubjectName
                                                        </td>

                                                    </tr>
                                                    SNo++;
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }
    
    else
    {

        <div class="col-xs-12">
            <span style="font-weight:bold;color:red">No records found</span>
        </div>
    }
</div>

@Html.Action("DataTableScripts", "Base", new { IsPaging = false, AllowScroll = true })


