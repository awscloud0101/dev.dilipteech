                                <p>Mobile  :<strong> @Model.FatherMobileNumber/@Model.FatherMobileNumber2</strong></p>
                                <p>Email :<strong> @Model.FatherEmail</strong></p>
                            </div>
                            <div class="col-sm-6" style="border-left:1px solid #ccc;">
                                <p>Qualification :<strong> @Model.FatherQualification</strong></p>
                                <p>Occupation	:<strong>@Model.FatherOccupation</strong></p>
                                <p>CompanyName	:<strong>@Model.FCompanyName</strong></p>
                                <p>	Designation	:<strong>@Model.FCompanyName</strong></p>
                            </div>
                        </div>
                    </div>

                    <div id="Mother" class="tabcontent col-sm-8" style="margin-top:12px;">

                        <div class="col-sm-3">
                            <span class="profile-picture">

                                @if (null != ViewBag.MPhoto && ViewBag.MPhoto != "")
                                {
                                    <img id="Mimage" src="@ViewBag.MPhoto" alt="your image" width="100" height="100" />
                                }
                                else
                                {
                                    <img id="Mimage" src="~/Content/images/NoPhoto.jpg" alt="your image" width="100" height="100" />

                                }


                            </span>

                        </div>





                        <div class="col-sm-9" style="border-left:1px solid #ccc;" align="left">
                            <h5 align="center" style="margin:0px;"><strong>Mother Information</strong></h5>
                            <hr />
                            <div class="col-sm-6">
                                <p>  Mother Name :	<strong>	@Model.MotherName</strong></p>
                                <p>Mobile  :<strong>@Model.MotherMobileNumber</strong></p>
                                <p>Email :<strong>@Model.MotherEmail</strong></p>
                            </div>
                            <div class="col-sm-6" style="border-left:1px solid #ccc;">
                                <p>Qualification :<strong> @Model.MotherQualification</strong></p>
                                <p>Occupation	:<strong> @Model.MotherOccupation</strong></p>
                                <p>CompanyName	:<strong>@Model.MCompanyName</strong></p>
                                <p>	Designation	:<strong>@Model.MDesignation</strong></p>
                            </div>





                        </div>
                    </div>




                    <div id="Guardian" class="tabcontent col-sm-8" style="margin-top:12px;">

                        <div class="col-sm-3">

                            <span class="profile-picture">
                                @if (null != ViewBag.GPhoto && ViewBag.GPhoto != "")
                                {
                                    <img id="Gimage" src="@ViewBag.GPhoto" alt="your image" width="100" height="100" />
                                }
                                else
                                {
                                    <img id="Gimage" src="~/Content/images/NoPhoto.jpg" alt="your image" width="100" height="100" />
                                }

                            </span>









                        </div>

                        <div class="col-sm-9" style="border-left:1px solid #ccc;" align="left">
                            <h5 align="center" style="margin:0px;"><strong>Guardian Information</strong></h5>
                            <hr />
                            <div class="col-sm-6">
                                <p>  Guardian Name :	<strong>@Model.GuardianName</strong></p>
                                @*<p>Relation :<strong> @Model.GuardianRelation</strong></p>*@
                                <p>Mobile  :<strong>@Model.GardianMobile</strong></p>
                                <p>Email :<strong> demo@gmail.com</strong></p>
                            </div>
                            <div class="col-sm-6" style="border-left:1px solid #ccc;">
                                <p>Qualification :<strong> @Model.GardianOccupation</strong></p>

                            </div>



                            <style>
                                /* Style the buttons inside the tab */
                                .tab button {
                                    display: block;
                                    background-color: #FCFCFC;
                                    color: black;
                                    padding: 17px 11px;
                                    width: 100%;
                                    border: none;
                                    outline: none;
                                    text-align: left;
                                    cursor: pointer;
                                    transition: 0.3s;
                                    font-size: 18px;
                                    font-family: calibri;
                                    border-bottom: dotted 1px #85BCDA;
                                }

                                    /* Change background color of buttons on hover */
                                    .tab button:hover {
                                        background-color: #ddd;
                                    }

                                    /* Create an active/current "tab button" class */
                                    .tab button.active {
                                        background-color: #85BCDA;
                                        color: white;
                                    }

                                /* Style the tab content */
                                .tabcontent {
                                    float: left;
                                    height: auto;
                                    background: #fff;
                                    padding: 0px;
                            </style>
                            <script>
                                function openparent(evt, parentName) {
                                    var i, tabcontent, tablinks;
                                    tabcontent = document.getElementsByClassName("tabcontent");
                                    for (i = 0; i < tabcontent.length; i++) {
                                        tabcontent[i].style.display = "none";
                                    }
                                    tablinks = document.getElementsByClassName("tablinks");
                                    for (i = 0; i < tablinks.length; i++) {
                                        tablinks[i].className = tablinks[i].className.replace(" active", "");
                                    }
                                    document.getElementById(parentName).style.display = "block";
                                    evt.currentTarget.className += " active";
                                }

                                // Get the element with id="defaultOpen" and click on it
                                document.getElementById("defaultOpen").click();
                            </script>

                        </div>
                    </div>
                </div>

            </div>

            <div id="payment" class="tab-pane" style="background:#fff;margin-top:20px;border-bottom:2px #85BCDA solid;">
                <div id="tab3" class="tabContent" style="margin-top:25px;height:500px;">
                    @if (null != Model.FeepaymentHistory && Model.FeepaymentHistory.Count() >= 1)
                    {
                        Sno = 1;
                        <div class="box-body">
                            <div class="box-body table-responsive no-padding">
                                <table class="table table-bordered table-striped table-hover nowrap">
                                    <thead>
                                        <tr>
                                            <th>
                                                S.No.
                                            </th>
                                            <th>
                                                @Html.DisplayName("Particular")
                                            </th>

                                            <th>
                                                @Html.DisplayName("Transaction Date")
                                            </th>
                                            <th>
                                                @Html.DisplayName("Receipt Number")
                                            </th>
                                            <th>
                                                @Html.DisplayName("Template Number")
                                            </th>
                                            <th>
                                                @Html.DisplayName("Amount")
                                            </th>
                                        </tr>

                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model.FeepaymentHistory)
                                        {
                                            <tr>
                                                <td>
                                                    @Sno
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.FeeParticularName)
                                                </td>

                                                <td>
                                                    @item.TransactionDate.Value.ToString("dd - MMM - yyyy")
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ReceiptNo)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.TemplateNumner)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.DebitAmount)
                                                </td>

                                            </tr>
                                            Sno++;
                                        }
                                    </tbody>

                                </table>
                            </div>

                        </div>
                    }

                </div>
            </div>


            <div id="fee" class="tab-pane" style="background:#fff;margin-top:20px;border-bottom:2px #85BCDA solid;">
                <div id="tab4" class="tabContent" style="margin-top:25px;height:500px;">
                    @if (null != Model.Feedetails && Model.Feedetails.Count() >= 1)
                    {
                        Sno = 1;
                        <div class="box-body">
                            <div class="box-body table-responsive no-padding">
                                <table class="table table-bordered table-striped table-hover nowrap">
                                    <thead>
                                        <tr>
                                            <th>
                                                SNo
                                            </th>
                                            <th>
                                                Particular Name
                                            </th>
                                            <th class="centerAlign">
                                                Fee Amount
                                            </th>
                                            <th class="centerAlign">
                                                Concession Amount
                                            </th>
                                            <th class="centerAlign">
                                                Net Amount
                                            </th>

                                            <th class="centerAlign">
                                                Paid Amount
                                            </th>

                                            <th class="centerAlign">
                                                Due Amount
                                            </th>

                                        </tr>
                                    </thead>
                                    <tbody>

                                        @for (int i = 0; i < Model.Feedetails.Count(); i++)
                                        {
                                            string color = "";
                                            if (Model.Feedetails[i].Status == "good")
                                            {
                                                color = "green";

                                                if (Model.Feedetails[i].BackgroundValue < 0)
                                                {
                                                    color = "lightyellow;";
                                                }
                                            }
                                            else
                                            {
                                                color = "red";

                                                if (Model.Feedetails[i].BackgroundValue < 0)
                                                {
                                                    color = "lightyellow;";
                                                }
                                            }

                                            <tr style="background-color:@color">
                                                <td>
                                                    @(Sno++)
                                                </td>
                                                <td>
                                                    @Model.Feedetails[i].FeeParticularName
                                                </td>
                                                <td>
                                                    @Model.Feedetails[i].FeeAmount.ToString()
                                                </td>
                                                <td>
                                                    @Model.Feedetails[i].DiscountAmount.ToString()
                                                </td>
                                                <td>
                                                    @Model.Feedetails[i].NetAmount.ToString()


                                                <td>
                                                    @Model.Feedetails[i].PaidAmount.ToString()
                                                </td>

                                                <td>
                                                    @Model.Feedetails[i].DueAmount.ToString()
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>

                            </div>

                        </div>
                    }

                </div>
            </div>

