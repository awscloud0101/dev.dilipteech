@model AsteriskBrowserAL.Models.StudentAdmissionData

@{
    ViewBag.Title = "Student Profile";
    int Sno = 1;

}
<style>
    #tabs {
        overflow: auto;
        width: 100%;
        list-style: none;
        margin: 0;
        padding: 0;
    }

        #tabs li {
            margin: 0;
            padding: 0;
            float: left;
        }

        #tabs a {
            -moz-box-shadow: -4px 0 0 rgba(0, 0, 0, .2);
            -webkit-box-shadow: -4px 0 0 rgba(0, 0, 0, .2);
            box-shadow: -4px 0 0 rgba(0, 0, 0, .2);
            background: #3c8dbc;
            background: -moz-linear-gradient(220deg, transparent 10px, #3c8dbc 10px);
            background: -webkit-linear-gradient(220deg, transparent 10px, #3c8dbc 10px);
            background: -o-linear-gradient(220deg, transparent 10px, #3c8dbc 10px);
            background: linear-gradient(220deg, transparent 10px, #3c8dbc 10px);
            text-shadow: 0 1px 0 rgba(0,0,0,.5);
            color: #fff;
            float: left;
            font: bold 12px/35px 'Lucida sans', Arial, Helvetica;
            height: 35px;
            padding: 0 30px;
            text-decoration: none;
        }





        #tabs #current a {
            background: #fff;
            background: -moz-linear-gradient(220deg, transparent 10px, #fff 10px);
            background: -webkit-linear-gradient(220deg, transparent 10px, #fff 10px);
            background: -o-linear-gradient(220deg, transparent 10px, #fff 10px);
            background: linear-gradient(220deg, transparent 10px, #fff 10px);
            text-shadow: none;
            color: #333;
        }


    #content {
        background-color: white;
        background-image: -webkit-gradient(linear, left top, left bottom, from(#fff), to(#ddd));
        background-image: -webkit-linear-gradient(top, white, #d8e8f7);
        background-image: -moz-linear-gradient(top, white, #d8e8f7);
        background-image: -o-linear-gradient(top, white, #d8e8f7);
        background-image: linear-gradient(top, white, #d8e8f7);
        -moz-border-radius: 0 2px 2px 2px;
        -webkit-border-radius: 0 2px 2px 2px;
        border-radius: 0 2px 2px 2px;
        -moz-box-shadow: 0 2px 2px #000, 0 -1px 0 white inset;
        -webkit-box-shadow: 0 2px 2px #000, 0 -1px 0 white inset;
        box-shadow: 0 2px 2px #000, 0 -1px 0 #fff inset;
        padding: 30px;
    }

    /* Remove the rule below if you want the content to be "organic" */
</style>

<script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>

@section BreadCrumb{

    <ul class="breadcrumb">
        <li>
            <i class="menu-icon fa fa-graduation-cap"></i>
            <a href="#">Academics</a>
        </li>
        <li>
            <i class=""></i>
            Student
        </li>
        <li class="active">Student Details</li>
    </ul><!-- /.breadcrumb -->
}


<section class="content">

    <div class="table-header">

        Student Details

        @if (null != Model)
        {
            if (!Model.IsParent)
            {
                if (!Model.IsParent)
                {
                    <small style="float:right;">
                        @Html.ActionLink("Back to details", "StudentDetails")
                    </small>
                }
            }
        }

    </div>
    <div class="tabbable">
        <ul class="nav nav-tabs padding-12 tab-color-blue background-blue" id="myTab4">

            <li class="active">
                <a data-toggle="tab" href="#student" aria-expanded="true">Profile</a>
            </li>

            <li class="">
                <a data-toggle="tab" href="#parent" aria-expanded="false">Parent Details</a>
            </li>

            <li class="">
                <a data-toggle="tab" href="#payment" aria-expanded="false">Payment History</a>
            </li>
            <li class="">
                <a data-toggle="tab" href="#fee" aria-expanded="false">Fee Details</a>
            </li>
            <li class="">
                <a data-toggle="tab" href="#progresscard" aria-expanded="false">Academic Performance</a>
            </li>
            <li class="">
                <a data-toggle="tab" href="#Academics" aria-expanded="false">Academic Analysis</a>
            </li>
            <li class="">
                <a data-toggle="tab" href="#Accomplishment" aria-expanded="false">Accomplishment</a>
            </li>
            <li class="">
                <a data-toggle="tab" href="#Attendance" aria-expanded="false">Attendance</a>
            </li>
        </ul>

        <div class="tab-content">

            <div id="student" class="tab-pane active" style="background:#fff;margin-top:20px;border-bottom:2px #85BCDA solid;">


                <div class="row" style="padding:15px;height:500px;">

                    <div class="col-sm-3" align="center">
                        <span class="profile-picture">

                            @if (Model.Photo != null && Model.Photo != "")
                            {
                                <img id="Simage" src="@Model.Photo" alt="your image" width="100" height="100" />
                            }
                            else
                            {
                                <img id="Simage" src="~/Content/images/NoPhoto.jpg" alt="your image" width="100" height="100" />
                            }

                        </span>

                        <div class="space space-4"></div>

                        <div class="width-80 label label-info label-xlg arrowed-in arrowed-in-right">
                            <div class="inline position-relative">
                                <span class="white">@Model.FirstName @Model.LastName</span>
                            </div>
                        </div>
                    </div>
                    <!-- /.col -->

                    <div class="col-xs-9">
                        <!-- #section:pages/profile.info -->

                        <div class="col-sm-9 font3" align="left" style="color:#333;">
                            @if (Model.Status == "good")
                            {
                                <h2><i class="ace-icon fa fa-circle light-green"> @Model.FirstName @Model.LastName </i></h2>
                            }
                            else if (Model.Status == "poor")
                            {
                                <h2><i class="ace-icon fa fa-circle light-red"> @Model.FirstName @Model.LastName </i></h2>
                            }
                            else if (Model.Status == "avg")
                            {
                                <h2><i class="ace-icon fa fa-circle light-orange"> @Model.FirstName @Model.LastName </i></h2>
                            }


                            <hr />

                            <div class="col-sm-5" align="left">
                                <table width="100%" border="0" cellspacing="0">
                                    <tr>
                                        <td class="blocktable2">Academic Year</td>
                                        <td class="blocktable2"><strong>@Model.DisplayAcademicYear</strong></td>
                                    </tr>
                                    <tr>



                                        <td class="blocktable1">Admission #	</td>
                                        <td class="blocktable2"><strong>@Model.AdmissionNumber</strong></td>
                                    </tr>
                                    <tr>



                                        <td class="blocktable1">Admission Date</td>
                                        <td class="blocktable2"><strong>@Model.AdmissionDate.Value.ToString("dd - MMM - yyyy")</strong></td>
                                    </tr>
                                    <tr>



                                        <td class="blocktable1">Class</td>
                                        <td class="blocktable2"><strong>@Model.ClassName</strong></td>
                                    </tr>
                                    <tr>
                                        <td class="blocktable1">Date of Birth</td>
                                        <td class="blocktable2">
                                            <strong>
                                                @if (Model.DateofBirth.HasValue)
                                                {
                                                    @Model.DateofBirth.Value.ToString("dd - MMM - yyyy")
                                                }
                                            </strong>
                                        </td>
                                    </tr>
                                    <tr>

                                        <td class="blocktable1">Roll Number </td>
                                        <td class="blocktable2">
                                            <strong>
                                                @if (Model.RollNumber != null)
                                                {
                                                    @Model.RollNumber.Value
                                                }
                                            </strong>
                                        </td>
                                    </tr>
                                    <tr>



                                        <td class="blocktable1">Adhar Card</td>
                                        <td class="blocktable2"><strong>@Model.AadharCardNo</strong></td>
                                    </tr>
                                    <tr>



                                        <td class="blocktable1">Stores Concession</td>
                                        <td class="blocktable2"><strong>@Model.StoresStatus</strong></td>
                                    </tr>



                                </table>

                            </div>


                            <div class="col-sm-7" style="border-left:1px solid #ccc;">
                                <p>  Address Line 1 :	<strong>@Model.AddressLineOne </strong></p>
                                <p>  Address Line 2 :	<strong> @Model.AddressLineTwo</strong></p>
                                <p>City :<strong> @Model.City</strong></p>
                                <p>State :<strong>@Model.StateName</strong></p>
                                <p>Pincode :<strong>@Model.PINCode</strong></p>
                                <p>Annual Income :<strong>@Model.AnnualIncome</strong></p>
                            </div>






                        </div>



                    </div>

                </div>
            </div>
            <div id="parent" class="tab-pane" style="background:#fff;margin-top:20px;border-bottom:2px #85BCDA solid;">
                <div class="row">
                    <div class="col-sm-3" style="margin-top:12px;height:500px; ">
                        <div class="tab">
                            <button class="tablinks" onclick="openparent(event, 'Father')" id="defaultOpen">Father Information</button>
                            <button class="tablinks" onclick="openparent(event, 'Mother')">Mother Information</button>
                            <button class="tablinks" onclick="openparent(event, 'Guardian')">Guardian Information</button>
                        </div>
                    </div>
                    <div id="Father" class="tabcontent col-sm-8" style="margin-top:12px;">

