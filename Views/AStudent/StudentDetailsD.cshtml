                                {
                                    <img id="Fimage" src="@ViewBag.FPhoto" alt="your image" width="100" height="100" />
                                }
                                else
                                {
                                    <img id="Fimage" src="~/Content/images/NoPhoto.jpg" alt="your image" width="100" height="100" />
                                }

