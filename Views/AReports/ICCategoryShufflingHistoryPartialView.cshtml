@model List<AsteriskBrowserAL.Models.StudentDetailsData>
@{
    int sno = 1;
    string style = string.Empty;
}

<div class="hr hr-18 dotted hr-double"></div>

<div class="row">
    <div class="col-xs-12">

        @if (null != Model && Model.Count() > 0)
            {
            <div class="table-header">
                Records Found(@Model.Count())
                <div style="float:right">
                    <button type="submit" class="btn btn-info btn-minier">
                        Export Excel
                    </button>
                </div>
            </div><!-- /.box-header -->

            <table class="table table-striped table-bordered table-hover dynamic-table">
                <thead>
                    <tr>
                        <th>
                            @("S.No")
                        </th>
                        <th>
                            @("Branch Name")
                        </th>
                        <th>
                            @Html.DisplayName("Student Name")
                        </th>
                        <th>
                            @Html.DisplayName("From Category")
                        </th>
                        <th>
                            @Html.DisplayName("To Category")
                        </th>
                        <th>
                            @Html.DisplayName("Transfer Date")
                        </th>
                    </tr>

                </thead>
                <tbody>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @(sno++)
                            </td>
                            <td>
                                @item.BranchName
                            </td>
                            <td>
                                @item.Name
                            </td>

                            <td>
                                @item.FromCategoryName
                            </td>
                            <td>
                                @item.ToCategoryName
                            </td>
                            <td>
                                @item.DisplayTransferDate
                            </td>
                        </tr>
                    }

                </tbody>
            </table>
            @Html.Action("DataTableScripts", "Base")

        }
        else
        {
            <div style="color:red;font-weight:bold">Data not available to display</div>
        }

    </div>


</div>





