@model AsteriskBrowserAL.Module
<section class="content-header">
    <ol class="breadcrumb">
        <li>
            <i class="menu-icon fa fa-list"></i>
            Settings
        </li>
        <li>
            <i class=""></i>
            CUserManagment
        </li>
        <li class="active">Module</li>
    </ol>
</section>
<section class="content">
    
    <div class="row">
        <div class="col-xs-12">
            <h3>@Html.ActionLink("<<Module Details", "Module")</h3>
        </div>
    </div>
    
    <div class="row">
        <div class="col-xs-12">
            @using (Html.BeginForm("ModuleD", "CUserManagement", FormMethod.Post, new { @class = "form-horizontal" }))
            {
                <div>
                    <span style="color: red;">
                        <b>Are you sure you want to delete this?</b>
                    </span>

                </div>
              @Html.HiddenFor(model=>model.ModuleId)
                <div class="box">

                    <div class="box-header">
                       
                     <button class="btn btn-primary pull-right" type="submit" style="width:80px;float:right;">
                            Delete
                        </button>
                    </div><!-- /.box-header -->
                    <!-- form start -->

                    <div class="box-body table-responsive no-padding">
                        <table class="table table-hover table-striped table-bordered">
                            <tbody>
                                <tr>
                                    <th>
                                       S.No.
                                        </th>
                                    <th>
                                        @Html.DisplayName("ModuleName")
                                    </th>
                                    <th>
                                        @Html.DisplayName("MIcon")
                                    </th>
                                    <th>
                                        @Html.DisplayName("Order Id")
                                        </th>
                                </tr>

                                <tr>
                                    <td>1</td>
                                    <td>
                                        @Html.DisplayFor(model => model.ModuleName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(model=>model.MIcon)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(model=>model.MOrderId)
                                        </td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
            }
        </div>
    </div>
