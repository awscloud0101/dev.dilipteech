<script>


    window.onload = GetData()
    function GetData() {
        debugger
        $('#SearchViewDiv').hide()
        LoadCourseDropdown();
        var PublicationId = $('#PublicationId').val()
        var AuthorId = $('#AuthorId').val()
        var BookDetailsId = $('#BookDetailsId').val()
        var MasterBookId = $('#MasterBookId').val()
        var DeptId = $('#DeptId').val();
        var SectionId = $('#SectionId').val();
        var UserCategoryId = $('#UserCategoryId').val()
        //if (UserCategoryId == "") {
        //     UserCategoryId = $('#UserCategoryId').val(-1)
        //}
        if (PublicationId != "" || AuthorId != "" || BookDetailsId != "" || DeptId != null || SectionId != null || UserCategoryId != "" || MasterBookId != "") {
            $.ajax({
                url: '/ALibrary/ReturnBookPartial',
                type: 'POST',
                data: { BookDetailsId: BookDetailsId, PublicationId: PublicationId, UserCategoryId: UserCategoryId, SectionId: SectionId, DeptId: DeptId, AuthorId: AuthorId, MasterBookId: MasterBookId },
                success: function (data) {
                    debugger
                    $('#SearchViewDiv').show()
                    $('#SearchViewDiv').html(data);
                    $('#imagemodal').hide();
                },
                error: function () {
                    $('#imagemodal').hide();
                },
            }).done(function () {
                $('#imagemodal').hide();
            })
        }
    }
    function LoadCourseDropdown() {
        debugger
        $.ajax({
            url: '/AFrontOffice/GetClassData',
            success: function (result) {
                CourseList = $("#ClassId")
                CourseList.children().remove();
                CourseList.append($("<option>").val("").text("--Select--"))
                $.each(result, function (e, record) {
                    CourseList.append($("<option>").val(record.ClassNumber).text(record.ClassNumberDisplay))
                })
                $("#ClassId").append(CourseList)
            },
            async: false,
        })
    }
    $("#ClassId").change(function () {
        debugger
        $('#Search').html('')
        $('#Student').val('')
        $('#SectionId').val('');
        $('#UserCategoryId').val('').trigger('change.select2');
        $('#MasterBookId').val('').trigger('change.select2');
        $('#BookDetailsId').val('').trigger('change.select2');
        $('#SearchStudent').hide();
        $('#SearchTeacher').hide();
        $('#Date').val('');
        var Date = $('#Date').val()
        var ClassId = $("#ClassId").val();
        if (ClassId > 0) {
            GetSectionDropDown(ClassId);
        }
        else {
        }
    })

</script>
<script>
    function GetSectionDropDown(ClassNumber) {

        $.ajax({
            url: '/AFrontOffice/GetSectionDropDown',
            data: { nClassNumber: ClassNumber },
            success: function (Data) {
                var SectionDRP = $('#SectionId')
                SectionDRP.children().remove();
                SectionDRP.append($("<option>").val("").text("--Select--"))
                $.each(Data, function (i, record) {
                    SectionDRP.append($("<option>").val(record.SectionId).text(record.SectionCode));
                });
                $('#SectionId').append(SectionDRP);
            },
            async: false,
        })
    }

</script>
<script>
    $("#BookDetailsId").autocomplete(
        {

            source: function (request, response) {

                debugger

                if (request.term.length < 0) {
                    return;
                }
                $("#MasterBookId").val('');
                $('#imagemodal').show();
                $('#UserCategoryId').val('').trigger('change.select2');
                $('#SearchStudent').hide();
                $('#SearchTeacher').hide();
                $('#Student').val('')
                $('#SectionId').val('');
                $('#ClassId').val('');
                $('#Date').val('');

                $.ajax({
                    url: "/ALibrary/GetBookCodeForReturn",
                    type: "POST",
                    dataType: "json",
                    data: {
                        strVal: request.term
                    },
                    success: function (json) {
                        debugger;
                        response($.map(json, function (jsondata, id) {
                            return {
                                label: jsondata.BookCode,
                                value: jsondata.BookCode,
                                id: jsondata.BookDetailsId,
                            };
                        }));
                        $('#imagemodal').hide();
                    },
                });
            },
            minLength: 1,
            select: function SelectEvent(event, ui) {

