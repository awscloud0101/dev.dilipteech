@model IEnumerable<AsteriskBrowserAL.Models.MagazineDetailsData>
<!-- #section:basics/content.breadcrumbs -->
<section class="content-header">
    <ol class="breadcrumb">
        <li>
            <i class="menu-icon fa fa-list"></i>
            Academics
        </li>
        <li>
            <i class=""></i>
            Library
        </li>
        <li class="active">Over All Magazine</li>
    </ol>
    <!-- /section:basics/content.breadcrumbs -->
</section>



@if (Model != null)
{
    if (Model.Count() > 0)
    {

        <div class="hr hr-18 dotted hr-double"></div>
        int nsno = 1;
        <div class="table-header">
            Records Found (@Model.Count())

        </div>

        <table id="mytab1" class="table table-hover table-bordered table-striped nowrap dynamic-table" style="font-size:14px;">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayName("SNo")
                    </th>
                    <th>
                        @Html.DisplayName("Magazine Code")
                    </th>
                    <th>
                        @Html.DisplayName("Magazine Category Name")
                    </th>
                    <th>
                        @Html.DisplayName("Magazine Name")
                    </th>
                    <th>
                        @Html.DisplayName("Edition")
                    </th>
                    <th>
                        @Html.DisplayName("Publisher")
                    </th>
                    <th>
                        @Html.DisplayName("Periodicity")
                    </th>
                    <th>
                        @Html.DisplayName("Price")
                    </th>
                   
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @(nsno++)
                        </td>
                        <td>
                            @item.MagazineCode
                        </td>
                        <td>
                            @item.MagazineCategoryName
                        </td>
                        <td>
                            @item.MagazineName
                        </td>
                        <td>
                            @item.Edition
                        </td>
                        <td>
                            @item.PublicationName
                        </td>

                        <td>
                            @item.PeriodicityName
                        </td>
                        <td>
                            @item.Price
                        </td>
                       
                    </tr>

                }

                @*<input type="hidden" id="BookCategoryId" name="nBookCategoryId" value="@Model.FirstOrDefault().BookCategoryId" />*@
            </tbody>
        </table>

    }
    else
    {
        <span style="color: red;"><b>No records to display</b></span>
    }
}

@Html.Action("DataTableScripts", "Base")




