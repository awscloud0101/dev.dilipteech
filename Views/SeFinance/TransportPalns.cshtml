                                                            @Html.DisplayName("Plan 9")
                                                        </th>
                                                        <th>
                                                            @Html.DisplayName("Plan 10")
                                                        </th>


                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @for (int i = 0; i < Model.Count(); i++)
                                                    {
                                                        <tr>
                                                            <td>
                                                                @Html.Hidden("AllTransShedulesList.Index", (@i + 10))
                                                                @sno
                                                                @Html.Hidden("AllTransShedulesList[" + (@i + 10) + "].TransportAmountId", Model.ElementAt(i).TransportAmountId)
                                                                @Html.Hidden("AllTransShedulesList[" + (@i + 10) + "].TransportPlanId", Model.ElementAt(i).TransportPlanId)
                                                                @Html.Hidden("AllTransShedulesList[" + (@i + 10) + "].SubscriptionId", Model.ElementAt(i).SubscriptionId)
                                                            </td>
                                                            <td id="branchCode">
                                                                @Model.ElementAt(i).Branch
                                                                <input type="hidden" id="branchCodenew" value="@Model.ElementAt(i).SubscriptionId">
                                                            </td>
                                                            <td>

                                                                @Html.TextBox("AllTransShedulesList[" + (@i + 10) + "].Plan1Aomunt", Model.ElementAt(i).Plan1Aomunt, new { @id = "paln1", @min = "0" })
                                                            </td>

                                                            <td>
                                                                @Html.TextBox("AllTransShedulesList[" + (@i + 10) + "].Plan2Aomunt", Model.ElementAt(i).Plan2Aomunt, new { @id = "plan2", @min = "0" })
                                                            </td>
                                                            <td>
                                                                @Html.TextBox("AllTransShedulesList[" + (@i + 10) + "].Plan3Aomunt", Model.ElementAt(i).Plan3Aomunt, new { @id = "plan3", @min = "0" })
