                                                            </td>
                                                            <td>
                                                                @Html.TextBox("AllTransShedulesList[" + (@i + 10) + "].Plan5Aomunt", Model.ElementAt(i).Plan5Aomunt, new { @id = "plan5", @min = "0" })
                                                            </td>
                                                            <td>
                                                                @Html.TextBox("AllTransShedulesList[" + (@i + 10) + "].Plan6Aomunt", Model.ElementAt(i).Plan6Aomunt, new { @id = "plan6", @min = "0" })
                                                            </td>
                                                            <td>
                                                                @Html.TextBox("AllTransShedulesList[" + (@i + 10) + "].Plan7Aomunt", Model.ElementAt(i).Plan7Aomunt, new { @id = "plan7", @min = "0" })
                                                            </td>
                                                            <td>
                                                                @Html.TextBox("AllTransShedulesList[" + (@i + 10) + "].Plan8Aomunt", Model.ElementAt(i).Plan8Aomunt, new { @id = "plan8", @min = "0" })
                                                            </td>
                                                            <td>
                                                                @Html.TextBox("AllTransShedulesList[" + (@i + 10) + "].Plan9Aomunt", Model.ElementAt(i).Plan9Aomunt, new { @id = "plan9", @min = "0" })
                                                            </td>
                                                            <td>
                                                                @Html.TextBox("AllTransShedulesList[" + (@i + 10) + "].Plan10Aomunt", Model.ElementAt(i).Plan10Aomunt, new { @id = "plan10", @min = "0" })
                                                            </td>
                                                        </tr>

                                                        sno++;
                                                    }


                                                </tbody>

                                            </table>

                                        </div>
                                    </div>
                                </div>
                            </div>
                    </div>



                }


            }

       

</section>
