
@model AsteriskBrowserAL.Models.StudentFeeRefundDataModel
@{
    int Sno = 1;
}



<div class="hr hr-18 dotted hr-double"></div>
@if (null != Model.IstStudentFeerefundDataList)
{

    foreach (var item in Model.IstStudentFeerefundDataList)
    {

        if (item.Level == 1 || item.Level == 0)
        {
            <div class="col-xs-12 col-sm-10 col-sm-offset-1">
                <div class="timeline-container">
                    <div class="timeline-label">
                        <span class="label label-success arrowed-in-right label-pink">
                            <b>Dean</b>
                        </span>
                    </div>

                    <div class="timeline-items">
                        <div class="timeline-item clearfix">

                            <div class="widget-box transparent">
                                <div class="widget-header widget-header-small">
                                    <h5 class="widget-title smaller"></h5>

                                    <span class="widget-toolbar no-border">
                                        <i class="ace-icon fa fa-clock-o bigger-110"></i>
                                        @item.LastUpdated.Value.ToString("dd - MMM - yyyy")
                                    </span>
                                    <span class="widget-toolbar">

                                    </span>
                                </div>

                                <div class="widget-body">
                                    <div class="widget-main">
                                        @item.Remarks

                                        <span class="blue bolder"></span>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div><!-- /.timeline-items -->
                </div>
            </div>
        }


        if (item.Level == 2 || item.Level == 0)
        {
            <div class="col-xs-12 col-sm-10 col-sm-offset-1">
                <div class="timeline-container">
                    <div class="timeline-label">
                        <span class="label label-success arrowed-in-right label-lg">
                            <b>Director</b>
                        </span>
                    </div>

                    <div class="timeline-items">
                        <div class="timeline-item clearfix">

                            <div class="widget-box transparent">
                                <div class="widget-header widget-header-small">
                                    <h5 class="widget-title smaller"></h5>

                                    <span class="widget-toolbar no-border">
                                        <i class="ace-icon fa fa-clock-o bigger-110"></i>
                                        @item.LastUpdated.Value.ToString("dd - MMM - yyyy")
                                    </span>
                                    <span class="widget-toolbar">
                                    </span>
                                </div>

                                <div class="widget-body">
                                    <div class="widget-main">
                                        @item.Remarks
                                        <span class="blue bolder"></span>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div><!-- /.timeline-items -->
                </div>
            </div>

        }

        if (item.Level == 3 || item.Level == 0)
        {
            <div class="col-xs-12 col-sm-10 col-sm-offset-1">
                <div class="timeline-container">
                    <div class="timeline-label">
                        <span class="label label-primary arrowed-in-right label-lg">
                            <b>BAT Head</b>
                        </span>
                    </div>

                    <div class="timeline-items">
                        <div class="timeline-item clearfix">
                            <div class="widget-box transparent">
                                <div class="widget-header widget-header-small">
                                    <h5 class="widget-title smaller"></h5>

                                    <span class="widget-toolbar no-border">
                                        <i class="ace-icon fa fa-clock-o bigger-110"></i>
                                        @item.LastUpdated.Value.ToString("dd - MMM - yyyy")
                                    </span>
                                    <span class="widget-toolbar">

                                    </span>
                                </div>

                                <div class="widget-body">
                                    <div class="widget-main">
                                        @item.Remarks

                                        <span class="blue bolder"></span>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div><!-- /.timeline-items -->
                </div>
            </div>
        }


    }
}