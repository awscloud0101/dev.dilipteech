@model AsteriskBrowserAL.HDExecutiveGroup
@{
    ViewBag.Title = "ActorRoleD";
    int sno = 1;
}

<!-- #section:basics/content.breadcrumbs -->
<section class="content-header">
    <ol class="breadcrumb">
        <li>
            <i class="menu-icon fa fa-list"></i>
            Settings
        </li>
        <li>
            <i class=""></i>
            Help Desk
        </li>
        <li class="active">Executive Group</li>
    </ol>
    <!-- /section:basics/content.breadcrumbs -->
</section>

<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <h3>@Html.ActionLink("<< Executive Group", "ExecutiveGroup")</h3>
        </div>
    </div>

    <div class="row">
        <div class="col-xs-12">
            @using (Html.BeginForm("ExecutiveGroupD", "SeHelpDesk", FormMethod.Post, new { @class = "form-horizontal" }))
            {
                <div>
                    <span style="color: red;">
                        <b>Are you sure you want to delete this?</b>
                    </span>
                </div>

                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title">Executive Group</h3>
                        <button class="btn btn-primary  pull-right" type="submit" style="width:80px;">
                            Delete
                        </button>
                    </div><!-- /.box-header -->
                    <!-- form start -->

                    <div class="box-body">
                        <table class="table table-hover">
                            <tbody>
                                <tr>
                                    <th>
                                       S.No.
                                    </th>
                                    <th>
                                        @Html.DisplayName("Group Name")
                                    </th>

                                </tr>

                                <tr>
                                    <td>
                                        @sno
                                    </td>
                                    <td>
                                        @Model.GroupName
                                    </td>

                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>

            }
        </div>
    </div>
</section>
