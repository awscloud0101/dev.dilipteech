@model IEnumerable<MVCAsterisk.FinanceExpense>

@{
    ViewBag.Title = "Expense";
}

<div class="page-content">
    <div class="col-xs-12">
        <div class="row">
            <!-- /.page-header -->
            <div class="page-header">
                <h1>Dashboard - @ViewBag.InstituteName
                </h1>
            </div>
            <!-- /.page-header -->
        </div>
    </div>
    <div class="col-xs-12">
        <div class="row">
            <table id="dynamic-table" class="table table-striped table-bordered table-hover">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Title)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Description)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Amount)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TransactionDate)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Voucher)
                    </th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Title)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Description)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Amount)
                        </td>
                        <td>
                            @item.TransactionDate.Value.ToString("dd-MMM-yyyy")
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Voucher)
                        </td>
                    </tr>
                }

            </table>

        </div>
    </div>

</div>

