@model AsteriskBrowserAL.Models.StudentAdmissionData
@{
    List<SelectListItem> PaymentMode = new List<SelectListItem>()
    {
        new SelectListItem()
        {
            Text = "Cash",
            Value = "1"
        },

        new SelectListItem()
        {
            Text = "Cheque",
            Value = "3"
        },

        new SelectListItem()
        {
            Text = "Swipe",
            Value = "5"
        },
         new SelectListItem()
        {
            Text = "Online",
            Value = "6"
        }
    };
}

<span style="color:red;font-weight:bold" id="TransportVal"></span>
<div class="row">
    <div class="col-xs-12">
        <div class="widget-box">

            <div class="widget-header widget-header-flat widget-header-small">

                <h5 class="widget-title">
                    Payments
                </h5>

                <div class="widget-toolbar no-border">
                    <div class="inline action-buttons">
                        <span id="PayButton">

                            <a name="Pay" value="Pay" id="Pay" onclick="Submit()"><span class="btn btn-info btn-minier">Pay</span></a>
                        </span>
                        <a id="Set" name="Set" value="Set"><span class="btn btn-info btn-minier">Set</span></a>
                    </div>
                </div>

                <br />
                <span id="ValPay" style="color:red"></span>

            </div>

            <div class="widget-body">
                <div class="widget-main">

                    <div class="row">
                        <div class="col-sm-6">
                            <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Admission Concession")</label>
                            <div class="col-sm-6" style="font:12px arial;">
                                <input class="form-control" id="AdmissionConcession" name="AdmissionDiscount" />
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Tution Fee Concession")</label>
                            <div class="col-sm-6" style="font:12px arial;">
                                <input class="form-control" id="ConcessionAmount"  name="ConcessionAmount" />
                            </div>
                        </div>

                    </div>
                    <br />
                    <br />
                    <div class="row">

                        <div class="col-sm-6">
                            <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Transport Plans")</label>
                            <div class="col-sm-6" style="font:12px arial;">
                                <select class="form-control " id="TransportPalnId" name="TransportPalnId"></select>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <label class="col-sm-5 no-padding-rights" for="form-field-1">@Html.Label("Initial Amount")</label>
                            <div class="col-xs-6">
                                <input class="form-control" id="InitialAmount" name="InitialAmount" />

                            </div>

                        </div>

                    </div>


                    @if (null != Model)
                    {
                        <input type="hidden" name="ClassNumber" id="ClassNumber" value="@Model.CourseId" />
                        <input type="hidden" name="AdmissionComponentId" id="AdmissionComponentId" value="@Model.AdmissionComponentId" />
                        <input type="hidden" name="AcademicYearId" id="SelAcademicYearId" value="@Model.AcademicYearId" />


                    }
                    <br />
                    <div class="row">
                        <div id="FeepaymentsPartial">
                            @Html.Partial("AdmissionFeepaymentsSubPartialView", Model)
                        </div>
                    </div>


                    <br />
                    <br />

                </div>
            </div>

        </div>

    </div>

</div>



