                                    <input type="hidden" id="LastReciept" value="@Model.OriginalManualReciept" />
                                    }
                                    else
                                    {
                                    <label class="control-label no-padding-right" for="form-field-1">@Html.Label("Reciept : (3)")</label>
                                    <input type="hidden" id="LastReciept" value="3" />
                                    }
                                </div>

                                <div class="col-sm-6">
                                    @if (null != Model)
                                    {

                                    <input class="form-control " id="ManualReciptNo" type="number" min="3" name="ManualReciptNo" value="@Model.OriginalManualReciept" />
                                    }
                                    else
                                    {
                                    <input class="form-control " id="ManualReciptNo" type="number" min="3" name="ManualReciptNo" />
                                    }
