@*Reciept Type*@
<div class="row">
    <div class="col-sm-12">
        <div class="widget-box">
            <div class="widget-header">
                <h5 class="widget-title">
                    Reciept
                </h5>
                <div class="widget-toolbar">
                    <a href="#" data-action="collapse">
                        <i class="ace-icon fa fa-chevron-up"></i>
                    </a>
                </div>
            </div>
            <div class="widget-body">
                <div class="widget-main">
                    <div class="row">

                        <div class="col-xs-6">

                            <label class="col-sm-5 control-label no-padding-right" for="form-field-1"></label>
                            <div class="col-xs-3">
                                <label><input type="radio" id="RecieptO" name="Reciept" value="1" checked="checked" /> Online</label>
                            </div>

                            <div class="col-xs-3">
                                <label><input type="radio" id="RecieptM" name="Reciept" value="2" /> Manual</label>
