
@model AsteriskBrowserAL.Models.StudentAdmissionData
@{
    DateTime TodayDate = DateTime.Today;

}
<style>
    .field-validation-error {
        color: red;
        font-weight: bold;
    }
</style>


@section BreadCrumb{
    <ul class="breadcrumb">
        <li>
            <i class="menu-icon fa fa-graduation-cap"></i>
            <a href="#">Academics</a>
        </li>
        <li>
            <i class=""></i>
            Front Office
        </li>
        <li class="active">Admission</li>
    </ul><!-- /.breadcrumb -->
}


<link href="~/assets/css/jquery-ui.custom.min.css" rel="stylesheet" />
<script src="~/assets/js/jquery-ui.min.js"></script>

<link href="http://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/css/datepicker.css" rel="stylesheet" type="text/css" />
<script src="http://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/js/bootstrap-datepicker.js"></script>

<link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/css/datepicker.css" rel="stylesheet" type="text/css" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/js/bootstrap-datepicker.js"></script>

<style type="text/css">
    .ui-autocomplete {
        font-size: 12px;
        cursor: pointer;
        width: 120px;
        height: 120px;
        overflow-y: scroll;
    }
</style>
<style>
    .required:first-letter {
        color: red;
        font-weight: bold;
    }
</style>

<form id="Submit" method="post" enctype="multipart/form-data">
    <section class="content">

        <span style="color:red;font-weight:bold" id="ErrorId">
        </span>
        @using (Html.BeginForm("AdmissionForm", "AFrontOffice", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div id="AdmissionView" style="display:inline">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="widget-box">
                            <div class="widget-header widget-header-flat widget-header-small">
                                <div class="widget-toolbar no-border">
                                    <div class="inline action-buttons">
                                        <div style="float:right;">
                                            <button class="btn btn-info btn-xs P2" name="Create" value="Create" type="submit" onclick="return submitForm()">
                                                <i class="ace-icon fa fa-floppy-o bigger-160"></i>
                                            </button>
                                        </div>

                                    </div>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>

                <!--Academic Year-->

                <div class="row">
                    <div class="col-xs-12">
                        <div class="widget-box">
                            <div class="widget-header">
                                <h5 class="widget-title">Academic Year</h5>
                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>
                            </div>

                            <div class="widget-body">
                                <div class="widget-main">

                                    <div class="row">
                                        <div class="col-xs-6">
                                            <label class="col-xs-5">@Html.Label("Academic Year")</label>
                                            <div class="col-xs-6">
                                                @Html.DropDownListFor(model => model.AcademicYearId, new SelectList(Model.lstAcademicYear, "AcademicYearId", "Name"), new { @class = "form-control textlength", id = "CboAcademicYearId", disabled = "disabled" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="space-4"></div>

                <!--Personal Information-->
                <div class="row">
                    <div class="col-xs-12">
                        <div class="widget-box">
                            <div class="widget-header">
                                <h5 class="widget-title">
                                    Personal Information
                                </h5>

                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>

                                <span style="color: red;font-weight:bold" id="PersonalVal"></span>

                            </div>

                            <div class="widget-body">
                                <div class="widget-main">
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("School Admission Number")</label>
                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.AccreditationNumber, new { @class = "form-control TextBox ", @Value = Model.AccreditationNumber, @style = "width:210px;", @maxlength = "10" })
                                                </div>

                                            </div>

                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right required" for="form-field-1">*Admission Date</label>

                                                <div class="col-sm-3">
                                                    <div class="row">
                                                        <div class="col-sm-12">
                                                            <div id="datepickerDate" style="width: 210px;" class="input-group date">
                                                                <input class="form-control" type="text" id="AdmissionDate" name="AdmissionDate" value="@TodayDate.ToShortDateString()" autocomplete="off" disabled="disabled" />
                                                                <span class="input-group-addon"><i class="ace-icon fa fa-calendar"></i></span>

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right required" for="form-field-1">*First Name</label>
                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.FirstName, new { @class = "form-control TextBox", @id = "FirstName", placeholder = "First Name", @style = "width:210px;", @maxlength = "20" })
                                                </div>
                                            </div>

                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right required" for="form-field-1">*Last Name</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.LastName, new { @class = "form-control TextBox", placeholder = "Last Name", @id = "LastName", title = "Please Select Last name", @maxlength = "20", @style = "width:210px;" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right required" for="form-field-1">*Class</label>

                                                <div class="col-sm-6">
                                                    <select class="form-control" style="width:210px;" id="CourseId" name="CourseId"></select>
                                                </div>
                                            </div>

                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right required" for="form-field-1">*Section</label>

                                                <div class="col-sm-6">
                                                    <select class="form-control" style="width:210px;" id="StudentBatchId" name="StudentBatchId"></select>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />

                                    <div class="row">
                                        <div class="col-xs-12">


                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right required" for="form-field-1">*Date of Birth</label>

                                                <div class="col-sm-3">
                                                    <div class="row">
                                                        <div class="col-sm-12">
                                                            <div id="datepickerDate" style="width: 210px;" class="input-group date">
                                                                <input class="form-control" type="text" id="DateofBirth" name="DateofBirth" autocomplete="off" placeholder="DateofBirth" />
                                                                <span class="input-group-addon"><i class="ace-icon fa fa-calendar"></i></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>

                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Gender")</label>
                                                <div class="col-xs-3">
                                                    <label>@Html.RadioButtonFor(m => m.Gender, 0, new { @name = "Male", @id = "Male", @checked = "checked" }) Male</label>
                                                </div>
                                                <div class="col-xs-3">
                                                    <label>@Html.RadioButtonFor(m => m.Gender, 1, new { @name = "Female", @id = "Female" }) Female</label>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right required" for="form-field-1">*Nationality</label>

                                                <div class="col-sm-6">
                                                    <select class="form-control" style="width:210px;" id="NationalityId" name="Nationality"></select>
                                                </div>
                                            </div>
                                            <div class="col-sm-6">

                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Religion")</label>

                                                <div class="col-sm-6">
                                                    <select class="form-control" style="width:210px;" id="Religion" name="Religion"></select>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">Caste</label>

                                                <div class="col-sm-6">
                                                    <select class="form-control" style="width:210px;" id="Caste" name="Caste"></select>
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Mother Tongue")</label>
                                                <div class="col-sm-6">
                                                    <select class="form-control" style="width:210px;" id="MotherTongue" name="MotherTongue"></select>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">Aadhar card No</label>

                                                <div class="col-sm-6">
                                                    <input style="width:210px" class="form-control" id="AadharCardNo" name="AadharCardNo" placeholder="Adhar card No" />

                                                </div>
                                            </div>
                                            <div class="col-sm-6">

                                                <label class="col-sm-5 control-label no-padding-right " for="form-field-1">Is Hostel</label>

                                                <div class="col-sm-6">

                                                    @Html.CheckBoxFor(model => model.IsHostel, new { @id = "Hostel" })
                                                    @*<input class="pull-left"  type="checkbox" id="IsHostel" name="IsHostel" value="false" />*@

                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-xs-12">
                                            @*<div class="col-sm-6">*@
                                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">@Html.Label("Identification Marks")</label>
                                            <div class="col-sm-9">

                                                @Html.TextAreaFor(model => model.Identificationmarks1, 1, 8, new { @class = "form-control", @maxlength = "100" })
                                            </div>
                                            @*</div>*@
                                        </div>
                                        <br />

                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col-xs-12">
                                            @*<div class="col-sm-6">*@
                                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1"></label>
                                            <div class="col-sm-9">

                                                @Html.TextAreaFor(model => model.Identificationmarks2, 1, 8, new { @class = "form-control", @maxlength = "100" })
                                            </div>
                                            @*</div>*@
                                        </div>
                                        <br />

                                    </div>
                                </div>
                            </div>
                            <br />

                        </div>

                    </div>
                </div>
                <br />

                <div class="space-4"></div>

                <!--Parent Information-->
                <div class="row">
                    <div class="col-xs-12">
                        <div class="widget-box">
                            <div class="widget-header">


                                <h5 class="widget-title">
                                    Parent Information
                                </h5>

                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>

                                <br />
                                <span style="color: red;font-weight:bold" id="ParentVal"></span>
                            </div>


                            <div class="widget-body">
                                <div class="widget-main">
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right required" for="form-field-1">*Father's Name</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.FatherName, new { @class = "form-control TextBox", @id = "FatherName", placeholder = "Father's Name", @title = "Please enter Father Name", @style = "width:210px;" })
                                                </div>
                                            </div>

                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right required" for="form-field-1">*Mother's Name</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.MotherName, new { @class = "form-control TextBox", @id = "MotherName", placeholder = "Mother's Name", title = "Please Select Mother's Name", @style = "width:210px;" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />

                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">Marriage Anniversary</label>

                                                <div class="col-sm-6">
                                                    <div class="row">
                                                        <div class="col-sm-12">
                                                            <div id="datepickerDate" style="width: 210px;" class="input-group date">
                                                                @if (@Model.ParentAnniversaryDate.HasValue)
                                                                {
                                                                    <input class="form-control" type="text" id="ParentAnniversaryDate" name="ParentAnniversaryDate" value="@Model.ParentAnniversaryDate.Value.ToShortDateString()" autocomplete="off" />
                                                                    <span class="input-group-addon"><i class="ace-icon fa fa-calendar"></i></span>

                                                                }
                                                                else
                                                                {
                                                                    <input class="form-control" type="text" id="ParentAnniversaryDate" name="ParentAnniversaryDate" autocomplete="off" placeholder="Anniversary Date" />
                                                                    <span class="input-group-addon"><i class="ace-icon fa fa-calendar"></i></span>

                                                                }
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>


                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">Father Qualification</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.FatherQualification, new { @class = "form-control TextBox", @id = "FatherQualification", placeholder = "Father Qualification", title = "Please Select Father Qualification", @style = "width:210px;" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />

                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Mother Qualification")</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.MotherQualification, new { @class = "form-control TextBox", @id = "MotherQualification", placeholder = "Mother Qualification", title = "Please Select Father Qualification", @style = "width:210px;" })
                                                </div>
                                            </div>

                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right required" for="form-field-1">*Father Occupation</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.FatherOccupation, new { @class = "form-control TextBox", @id = "FatherOccupation", placeholder = "Father Occupation", title = "Please Select Father Occupation", @style = "width:210px;" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />

                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Mother Occupation")</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.MotherOccupation, new { @class = "form-control TextBox", @id = "MotherOccupation", placeholder = "Mother Occupation", title = "Please Select Father Occupation", @style = "width:210px;" })
                                                </div>
                                            </div>

                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Father Company Name")</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.FCompanyName, new { @class = "form-control TextBox", @id = "FCompanyname", placeholder = "Father Company Name", title = "Please Select Father Company Name", @style = "width:210px;" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />

                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Mother Company Name")</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.MCompanyName, new { @class = "form-control TextBox", @id = "MCompanyname", placeholder = "Mother Company Name", title = "Please Select Mother Company Name", @style = "width:210px;" })
                                                </div>
                                            </div>

                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right required" for="form-field-1">*Father Designation</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.FDesignation, new { @class = "form-control TextBox", @id = "FDesignation", placeholder = "Father Designition", title = "Please Select Mother Company Name", @style = "width:210px;" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <br />
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Mother Designation")</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.MDesignation, new { @class = "form-control TextBox", @id = "MDesignation", placeholder = "Mother Designition", title = "Please Select Mother Designition", @style = "width:210px;" })
                                                </div>
                                            </div>


                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Father Office Address")</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.FOfficeAddress, new { @class = "form-control TextBox", @id = "FOfficeAddress", placeholder = "Father Office Address", title = "Please Select Father Office Address", @style = "width:210px;" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br />

                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Mother Office Address")</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.MOfficeAddress, new { @class = "form-control TextBox", @id = "MOfficeAddress", placeholder = "Mother Office Address", title = "Please Select Mother Office Address", @style = "width:210px;" })
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <label class="col-sm-5 control-label no-padding-right" for="form-field-1">@Html.Label("Annual Income")</label>

                                                <div class="col-sm-6">
                                                    @Html.TextBoxFor(model => model.AnnualIncome, new { @class = "form-control TextBox", @id = "AnnualIncome", placeholder = "Annual Income", title = "Please Select Annual Income", @style = "width:210px;" })
                                                </div>
                                            </div>


                                            <br />
                                            <br />
                                            <br />


                                        </div>
                                    </div>
                                    <br />


                                </div>

                            </div>



                        </div>
                    </div>
                </div>

                <div class="space-4"></div>
                <!--Uploads-->
                <div class="row">
                    <div class="col-xs-12">
                        <div class="widget-box">
                            <div class="widget-header widget-header-flat widget-header-small">
                                <h5 class="widget-title">
                                    Uploads
                                </h5>
                                <!-- tools box -->
                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="widget-body">
                                <div class="widget-main">
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-xs-6" style="background:#fff;border:dotted 1px grey;padding:10px;">

                                                <label class="col-xs-5">
                                                    @Html.Label("Student Photo")
                                                    <input type="file" class="choose" name="StudentPhotoDocs" style="margin-top:0.5em" onchange="readURL(this,1);" />

                                                </label>
                                                <div class="col-xs-6">
                                                    <img width="100" height="100" id="DisplayImage1" />

                                                </div>

                                            </div>
                                            <div class="col-xs-6" style="background:#fff;border:dotted 1px grey;padding:10px;">
                                                <label class="col-xs-5 ">
                                                    @Html.Label("Aadhar Card")
                                                    <input type="file" class="choose" name="AadhaarPhotoDocs" style="margin-top:0.5em" onchange="readURL(this,2);" />

                                                </label>

                                                <div class="col-xs-6">
                                                    <img width="100" height="100" id="DisplayImage2" />

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <br /><br />
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="col-xs-6" style="background:#fff;border:dotted 1px grey;padding:10px;">
                                                <label class="col-xs-5">
                                                    @Html.Label("Father Photo")
                                                    <input type="file" class="choose" name="ParentPhotoDocs" style="margin-top:0.5em" onchange="readURL(this,3);" />


                                                </label>
                                                <div class="col-xs-6">
                                                    <img width="100" height="100" id="DisplayImage3" />
                                                </div>
                                            </div>
                                            <div class="col-xs-6" style="background:#fff;border:dotted 1px grey;padding:10px;">
                                                <label class="col-xs-5">
                                                    @Html.Label("Mother Photo")
                                                    <input type="file" class="choose" name="MotherPhotoDocs" style="margin-top:0.5em" onchange="readURL(this,4);" />


                                                </label>
                                                <div class="col-xs-6">
                                                    <img width="100" height="100" id="DisplayImage4" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--siblings Information-->
                <div class="row">
                    <div class="col-xs-12">
                        <div class="widget-box">
                            <div class="widget-header">

                                <h5 class="widget-title">
                                    Siblings Information
                                </h5>

                                <div class="widget-toolbar">
                                    <a href="#" data-action="collapse">
                                        <i class="ace-icon fa fa-chevron-up"></i>
                                    </a>
                                </div>
                                <br />
