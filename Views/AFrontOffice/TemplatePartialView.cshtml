@model List<AsteriskBrowserAL.Models.TemplateData>
@{
    int sno = 1;
}



<div class="col-xs-12">

    <div class="hr hr-12 dotted hr-double"></div>

    <span id="RemarksValidation" style="color:red;font-weight:bold"></span>
    <div class="box">
        <div class="box-body">
            <table class="table table-hover table-striped table-bordered">
                <thead>
                    <tr style="background-color:burlywood">
                        <th>
                           S.No.
                        </th>
                        <th>
                            @Html.DisplayName("Template No")
                        </th>

                        <th>
                            @Html.DisplayName("Remarks")
                        </th>
                    </tr>
                </thead>
                <tbody id="tbTemplateReceipt" style="overflow: auto; height: 300px; ">
                    @if (null != Model)
                    {
                        for (int i = 0; i < Model.Count(); i++)
                        {
                            <tr>
                                <td>
                                    @Html.Hidden("AdmissionMissingTemplats.Index", (@i + 10))
                                    @sno
                                    @Html.Hidden("AdmissionMissingTemplats[" + (@i + 10) + "].TemplateReciept", Model.ElementAt(i).TemplateReciept)

                                </td>
                                <td>
                                    @Model.ElementAt(i).TemplateReciept
                                </td>
                                <td>
                                    @Html.TextBox("AdmissionMissingTemplats[" + (@i + 10) + "].Remarks", Model.ElementAt(i).Remarks, new { @id = "Remarks", @required = "required" })
                                    @Html.Hidden("AdmissionMissingTemplats[" + (@i + 10) + "].Remarks", Model.ElementAt(i).Remarks)
                                </td>

                            </tr>

                            sno++;
                        }
                    }
                </tbody>
            </table>
        </div>

    </div>

</div>

<script>

    function GetListOfMissingReceips()
    {
        var MissingReceipts = @Html.Raw(Json.Encode(Model));

        return MissingReceipts;
    }

</script>