@model AsteriskBrowserAL.Qualifications
@{
    ViewBag.Title = "Delete Qualifications";

}

@section BreadCrumb{

    <ul class="breadcrumb">
        <li>
            <i class="menu-icon fa fa-list"></i>
            Setting
        </li>
        <li>
            <i class=""></i>
            HR
        </li>
        <li class="active">Qulifications</li>
    </ul><!-- /.breadcrumb -->
}




<section class="content">
    <div class="row">
        <div class=col-xs-12>
            <h3>@Html.ActionLink("<< Qualifications Details", "Qualifications")</h3>
        </div>
    </div>
    <br />

    <div class="row">
        <div class="col-xs-12">
            @using (Html.BeginForm("QualificationsD", "SeHR", FormMethod.Post, new { @class = "form-horizontal" }))
            {
                <div>
                    <span style="color: red;">
                        <b>Are you sure you want to delete this?</b>
                    </span>
                </div>
                <div class="box">
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true)
                    <span style="color: red;">@Html.ValidationMessage("Qualifications")</span>
                    <div class="box-header">
                        <h3 class="box-title">Qualifications</h3>
                     
                        <button class="btn btn-primary pull-right" type="submit" style="width:80px;">
                            Delete
                        </button>
                    </div><!-- /.box-header -->
                    <!-- form start -->

                    <div class="box-body table-responsive no-padding">
                        <table class="table table-hover table-bordered table-striped">
                            <tbody>
                                <tr>
                                    <th>
                                       S.No.
                                    </th>
                                    <th>
                                        @Html.DisplayName("Qualification Name")
                                    </th>
                                </tr>

                                <tr>
                                    <td>
                                        1
                                    </td>
                                    <td>
                                        @Html.DisplayFor(model => model.Name)
                                    </td>

                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
            }
        </div>
    </div>
</section>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}






