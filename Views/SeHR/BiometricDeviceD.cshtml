@model AsteriskBrowserAL.BioMetricDevice

@{
    ViewBag.Title = "Delete Biometric Device";
    int nsno = 1;
}

@section BreadCrumb{

    <ul class="breadcrumb">
        <li>
            <i class="menu-icon fa fa-list"></i>
            Setting
        </li>
        <li>
            <i class=""></i>
            HR
        </li>
        <li class="active">Biometric Device</li>
    </ul><!-- /.breadcrumb -->
}





<section class="content">
    <div class="row">
        <div class=col-xs-12>
            <h3>@Html.ActionLink("<< Biometric Device Details", "BiometricDevice")</h3>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-xs-12">

            @using (Html.BeginForm("BiometricDeviceD", "SeHR", FormMethod.Post, new { @class = "form-horizontal" }))
            {
                <div class="box">
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true)
                    <span style="color: red;">@Html.ValidationMessage("BiometricDevice")</span>
                    <div class="box-body">
                        <div class="box-header">
                            <h3 class="box-title">Biometric Device</h3>
                            <br />
                            <br />
                            <button class="btn btn-primary" type="submit" style="width:80px;">
                                Delete
                            </button>
                        </div><!-- /.box-header -->
                        <!-- form start -->



                        <div class="box-body table-responsive no-padding">
                            <table class="table table-hover">
                                <tbody>
                                    <tr>

                                        <th>
                                           S.No.
                                        </th>
                                        <th>
                                            @Html.DisplayName("DeviecId")
                                        </th>
                                        <th>
                                            @Html.DisplayName("Branch Name")
                                        </th>
                                        <th>
                                            @Html.DisplayName("Enroll Start Number")
                                        </th>
                                        <th></th>
                                    </tr>
                                    <tr>
                                        <td>
                                            @(nsno++)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.BiometricDeviceId)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.BranchName)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.StartNumber)
                                        </td>
                                        <td>
                                    </tr>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            }
        </div>


    </div>
</section>






