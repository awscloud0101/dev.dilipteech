@model IEnumerable<AsteriskBrowserAL.Models.MonthlyStudyHourReportData>
@if (null != Model && Model.Count() > 0)
{
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <div>
                        <input class="btn btn-primary" style="width:80px;" type="button" value="Print " onclick=" GetDataPrint('#mydiv')" />
                    </div>
                </div><!-- /.box-header -->

                <div class="box-body table-responsive no-padding">
                    <table class="table table-hover">
                        <tbody>
                            <tr>
                                <th>
                                    @Html.DisplayName("Staff Name")
                                </th>
                                <th>
                                    @Html.DisplayName("Employee Number")
                                </th>
                                <th>
                                    @Html.DisplayName("Analysis")
                                </th>
                                <th>
                                    @Html.DisplayName("Date")
                                </th>
                            </tr>

                            @foreach (var item in Model)
                            {
                                <tr>

                                    <td>
                                        @Html.DisplayFor(modelItem => item.EmployeeName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.EmployeeNumber)
                                    </td>
                                    @*<td>
                                            @Html.DisplayFor(modelItem => item.Analysis)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.StaffName)
                                        </td>*@

                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
}

